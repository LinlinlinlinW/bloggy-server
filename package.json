{
    "author": "Chuntong Gao, Bolin Wang, Yang Song, Hongkaiyue Zhou",
    "dependencies": {
        "@types/jsonwebtoken": "^8.5.0",
        "babel-preset-env": "^1.7.0",
        "babel-register": "^6.26.0",
        "consola": "^2.12.1",
        "cookie-parser": "~1.4.3",
        "crypto": "^1.0.1",
        "debug": "~2.6.9",
        "dotenv": "^8.0.0",
        "ejs": "~2.5.7",
        "express": "~4.16.0",
        "express-session": "^1.15.6",
        "http-errors": "~1.6.2",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^3.1.8",
        "mongoose": "^5.3.7",
        "mongoose-auto-increment": "^5.0.1",
        "morgan": "~1.9.0",
        "node-fetch": "^2.3.0"
    },
    "description": "server code for our blog project",
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/consola": "^2.2.5",
        "@types/cookie-parser": "^1.4.2",
        "@types/debug": "^4.1.5",
        "@types/ejs": "^2.0.0",
        "@types/express": "^4.17.6",
        "@types/express-session": "^1.17.0",
        "@types/http-errors": "^1.6.3",
        "@types/mocha": "^7.0.2",
        "@types/mongodb": "^3.5.17",
        "@types/mongoose": "^5.7.19",
        "@types/mongoose-auto-increment": "^5.0.33",
        "@types/morgan": "^1.9.0",
        "@types/node-fetch": "^2.5.7",
        "@types/supertest": "^2.0.9",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "eslint-plugin-require-sort": "^1.2.1",
        "eslint-plugin-simple-import-sort": "^5.0.3",
        "husky": "^4.2.3",
        "lint-staged": "9.4.3",
        "mocha": "^7.1.2",
        "prettier": "^2.0.4",
        "supertest": "^4.0.2",
        "typescript": "^3.9.2"
    },
    "license": "ISC",
    "name": "blog-server",
    "scripts": {
        "build": "rm -rf build/ && npm run check && npm run tsc-build && cp -r src/views build/views",
        "check": "npm run prettier-check && npm run eslint-check && npm run tsc-check",
        "eslint-check": "eslint --no-error-on-unmatched-pattern src/** tests/** --ext .js --ext .ts",
        "eslint-fix": "eslint --no-error-on-unmatched-pattern src/** tests/** --ext .js --ext .ts --fix",
        "fix": "npm run prettier-fix && npm run eslint-fix",
        "prettier-check": "prettier --config ./.prettierrc.json --check \"./src/**/*.{js,ts}\" \"./tests/**/*.{js,ts}\"",
        "prettier-fix": "prettier --config ./.prettierrc.json --write \"./src/**/*.{js,ts}\" \"./tests/**/*.{js,ts}\"",
        "start-dev": "node build/src/index.js --no-deprecation --no-warning",
        "tsc-build": "tsc --listEmittedFiles",
        "tsc-check": "tsc --noEmit --listEmittedFiles",
        "test": "mocha 'build/tests/**/*.spec.js' --require tests/mocha.env.js"
    },
    "version": "1.0.0"
}
